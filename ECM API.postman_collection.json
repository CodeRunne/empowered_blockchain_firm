				"type": "text/javascript",
				"exec": [
					"pm.sendRequest({",
					"    url: 'http://localhost:8000/sanctum/csrf-cookie',",
					"    method: 'GET'",
					"}, function(error, response, {cookies}) {",
					"    pm.collectionVariables.set('csrf-token', cookies.get('XSRF-TOKEN'));",
					"});"
				]
			}
